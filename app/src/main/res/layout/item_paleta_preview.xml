<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:baselineAligned="false"
    android:orientation="horizontal" >



    <LinearLayout
        android:id="@+id/listaColorWrapper1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:id="@+id/listaColor1"
            android:layout_width="@dimen/caja_coloresPreview"
            android:layout_height="@dimen/caja_coloresPreview"
            android:layout_margin="@dimen/caja_coloresPreviewMargin"
            android:background="@color/blanco"
            android:orientation="horizontal" >
        </LinearLayout>

        <TextView
            android:id="@+id/codigo1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:textSize="@dimen/font_size_color_preview"
            android:layout_marginBottom="3dp"
            android:layout_marginLeft="3dp"
            android:layout_marginRight="3dp"/>
    </LinearLayout>

