            android:layout_marginBottom="@dimen/caja_coloresPreviewMargin"
            android:layout_marginRight="@dimen/caja_coloresPreviewMargin"
            android:background="@color/blanco"
            android:orientation="horizontal" >
        </LinearLayout>


        <TextView
